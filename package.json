{
  "name": "legal-workflow",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=22.14.0 < 23.0.0"
  },
  "main": "lib/index.js",
  "sideEffects": [
    "*.scss",
    "*.css",
    "**/node_modules/@pnp/**",
    "**/node_modules/spfx-toolkit/lib/utilities/context/pnpImports/**",
    "**/node_modules/devextreme/dist/css/**"
  ],
  "scripts": {
    "build": "gulp bundle",
    "serve": "fast-serve",
    "debug": "gulp clean && gulp bundle && gulp package-solution",
