    "clean": "gulp clean && gulp clean-cache",
    "stats": "gulp bundle --ship && gulp analyze-bundle",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
