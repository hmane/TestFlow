           Required="TRUE"
           Min="1"
           Max="30"
           Decimals="0"
           Group="Submission Items">
      <Default>5</Default>
    </Field>

    <Field Type="Note"
           DisplayName="Description"
           Name="Description"
           ID="{D817C894-3F4D-457A-AEB3-F6D73DF5D2B2}"
           Required="FALSE"
           RichText="FALSE"
